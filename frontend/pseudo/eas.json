{
    "cli": {
      "appVersionSource": "local"
    },
    "build": {
      "development": {
        "developmentClient": true,
        "distribution": "internal",
        "ios": {
          "simulator": true
        },
        "android": {
          "gradleCommand": ":app:assembleDebug",
          "buildType": "apk"
        }
      },
      "preview": {
        "distribution": "internal",
        "android": {
          "buildType": "apk",
          "gradleCommand": ":app:assembleRelease",
          "withoutCredentials": true
        }
      }
    }
  }