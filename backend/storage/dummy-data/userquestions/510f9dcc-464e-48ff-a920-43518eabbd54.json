            "lines": [
                {
                    "number": 1,
                    "text": "class Solution:"
                },
                {
                    "number": 2,
                    "text": "    def three_sum(self, nums: List[int]) -> List[List[int]]:"
                },
                {
                    "number": 3,
                    "text": "        nums.sort()"
                },
                {
                    "number": 4,
                    "text": "        result = []"
                },
                {
                    "number": 5,
                    "text": "        for i in range(len(nums)-2):"
                },
                {
                    "number": 6,
                    "text": "            if i > 0 and nums[i] == nums[i-1]:"
                },
                {
                    "number": 7,
                    "text": "                continue"
                },
                {
                    "number": 8,
                    "text": "            left, right = i+1, len(nums)-1"
                },
                {
                    "number": 9,
                    "text": "            while left < right:"
                },
                {
                    "number": 10,
                    "text": "                total = nums[i] + nums[left] + nums[right]"
                },
                {
                    "number": 11,
                    "text": "                if total == 0:"
                },
                {
                    "number": 12,
                    "text": "                    result.append([nums[i], nums[left], nums[right]])"
                },
                {
                    "number": 13,
                    "text": "                    while left < right and nums[left] == nums[left+1]:"
                },
                {
                    "number": 14,
                    "text": "                        left += 1"
                },
                {
                    "number": 15,
                    "text": "                    while left < right and nums[right] == nums[right-1]:"
                },
                {
                    "number": 16,
                    "text": "                        right -= 1"
                },
                {
                    "number": 17,
                    "text": "                    left += 1"
                },
                {
                    "number": 18,
                    "text": "                    right -= 1"
                },
                {
                    "number": 19,
                    "text": "                elif total < 0:"
                },
                {
                    "number": 20,
                    "text": "                    left += 1"
                },
                {
                    "number": 21,
                    "text": "                else:"
                },
                {
                    "number": 22,
                    "text": "                    right -= 1"
                },
                {
                    "number": 23,
                    "text": "        return result"
                }
            ]
        },
